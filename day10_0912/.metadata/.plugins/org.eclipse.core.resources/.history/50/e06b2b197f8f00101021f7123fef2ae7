package kr.co.inherit;

public class InheritClass {

	public InheritClass() {
		
		Dog dog = new Dog("호두", 3, "진돗개");
		
		Cat cat = new Cat("껌이", 5, true);

		System.out.println("이름: " + dog.name + ", 나이: " + dog.age + ", 종: " + dog.getBreeds());
		dog.displayInfo();
		dog.eat();
		dog.makeSound();
		dog.wagTail();
		
		System.out.println();
		
		System.out.println("이름: " + cat.name + ", 나이: " + cat.age + ", 산책여부: " + !cat.getIndoor());
		cat.displayInfo();
		cat.eat();
		cat.makeSound();
		cat.climb();
		
	}
}
