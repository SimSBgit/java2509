package kr.co.poly;

import kr.co.inherit.Animal;
import kr.co.inherit.Cat;
import kr.co.inherit.Dog;

public class PolymorphismClass {
	
	PolymorphismClass(){
		Animal[] animals = {
				new Dog("바둑이", 5, "진돗개"),
				new Cat("나비", 3, false),
				new Dog("체리", 2, "푸들"),
				new Cat("까망이", 1, true)
		};
		System.out.println("동물들의 소리");
		for(Animal animal: animals) {
			animal.makeSound();
		}
		
	}
	
	
	
}
